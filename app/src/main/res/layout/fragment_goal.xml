<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="bodygate.bcns.bodygation.navigationitem.GoalFragment">

    <!-- TODO: Update blank fragment layout -->

    <TextView
        android:id="@+id/goal_height_fitlbl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="3dp"
        android:paddingStart="3dp"
        android:text="@string/fitheghit" />

    <LinearLayout
        android:id="@+id/goal_height_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:paddingBottom="1dp"
        android:paddingTop="1dp">

        <TextView
            android:id="@+id/goal_height_lbl"
            android:layout_width="130dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingEnd="5dp"
            android:paddingLeft="3dp"
            android:text="@string/tall"
            android:textAlignment="viewEnd" />

        <EditText
            android:id="@+id/goal_height_txtB"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="2dp"
            android:layout_marginTop="2dp"
            android:layout_weight="1"
            android:autoText="false"
            android:ems="10"
            android:inputType="number"
            android:paddingStart="30dp"
            android:text="35"
            android:textAlignment="textStart"
            android:textSize="15sp" />
    </LinearLayout>

    <LinearLayout
